{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "endOfLine": "auto",
  "importOrder": [
    "^react$",
    "^src/modules/(.*)$",
    "^src/components/(.*)$",
    "^src/hooks/(.*)$",
    "^src/constants/(.*)$",
    "^src/api/(.*)$",
    "^src/store/(.*)$",
    "^src/utils/(.*)$",
    "^src/styles/(.*)$",
    "^src/router/(.*)$",
    "^src/assets/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
